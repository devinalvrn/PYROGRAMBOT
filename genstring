#-----WELCOME---#
from pyrogram import Client
import asyncio

API_ID = 
API_HASH = ""

app = Client(":memory:", api_id=API_ID, api_hash=API_HASH, in_memory=True)

async def main():
    await app.start()
    xx = await app.export_session_string()
    first_name = (await app.get_me()).first_name
    string_session = f"Halo {first_name}Ini Adalah String Pyrogram Kamu !\n<code>{await app.export_session_string()}</code>"
    await app.send_message("me", string_session)
    print(f"String has been sent to your saved messages: {first_name}")
    print(string_session)
    await app.stop()

app.run(main())

